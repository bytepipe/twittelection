#!/usr/bin/perl

use strict;
use warnings;
use 5.010;

use open IO => ':encoding(utf8)';

use FindBin qw[$Bin];

use lib 'lib';
use TwittElection::Schema;

my $sch = TwittElection::Schema->get_schema;

my $con_rs = $sch->resultset('Constituency');

for ($con_rs->all) {
  say $_->name;
  system "$Bin/get_cands", $_->mapit_id;
}

